{
  "extends": "expo/tsconfig.base",
  
  "compilerOptions": {
    "esModuleInterop": true,
    "target": "es5",
    "baseUrl": "./src",
    "lib": [
      "es2015",
      "esnext.asynciterable"
    ],
    "jsx": "react-native",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitAny": false,
    "useUnknownInCatchVariables": false,
    "types": [
      "react-native"
    ],
  },
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ]
}
